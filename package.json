{
  "name": "@skylib/framework",
  "version": "0.1.32",
  "private": false,
  "description": "Typescript framework",
  "keywords": [
    "framework",
    "typescript"
  ],
  "homepage": "https://github.com/ilyub/typescript-framework",
  "bugs": {
    "url": "https://github.com/ilyub/typescript-framework/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/typescript-framework.git"
  },
  "license": "ISC",
  "author": "Ilia Liubinskii <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "dist/index.js",
  "module": "es/index.js",
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "build-doc": "typedoc src",
    "build-es": "tsc --project tsconfig-build-es.json",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,fixtures,configs,src,tests}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "test": "jest",
    "test-fast": "jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,prod --reject @types/node",
    "update-package-check-peer": "ncu --dep peer --minimal --reject @types/node",
    "update-package-check-skylib": "ncu --dep peer --filter @skylib/*",
    "update-package-force": "ncu --dep dev,prod --reject @types/node --upgrade",
    "update-package-force-peer": "ncu --dep peer --minimal --reject @types/node --upgrade",
    "update-package-force-skylib": "ncu --dep peer --filter @skylib/* --upgrade"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@jest/test-sequencer": "^27.5.1",
    "@skylib/config": "^1.7.2",
    "@skylib/eslint-plugin": "^0.1.24",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "cpy-cli": "^4.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-boundaries": "^2.9.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-escompat": "^3.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsdoc": "^39.2.7",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-more": "^1.0.5",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.7.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-export-all": "^1.2.2",
    "eslint-plugin-tsdoc": "^0.2.16",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-xss": "^0.1.11",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-environment-node": "^27.5.1",
    "jest-mock-axios": "^4.6.1",
    "jest-transform-stub": "^2.0.0",
    "npm-check-updates": "^12.5.9",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4",
