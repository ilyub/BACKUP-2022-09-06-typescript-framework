    "lorem-ipsum": "^2.0.4",
    "lunr": "^2.3.9",
    "minisearch": "^4.0.3",
    "natural-compare": "^1.4.0",
    "pouchdb": "^7.3.0",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-collate": "^7.2.2",
    "sha256": "^0.2.0",
    "tslib": "^2.3.1",
