{
  "name": "@skylib/framework",
  "version": "0.1.17",
  "private": false,
  "description": "Typescript framework",
  "keywords": [
    "framework",
    "typescript"
  ],
  "homepage": "https://github.com/ilyub/typescript-framework",
  "bugs": {
    "url": "https://github.com/ilyub/typescript-framework/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/typescript-framework.git"
  },
  "license": "ISC",
  "author": "Ilya Lyubinskiy <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "scripts": {
    "build": "tsc --project tsconfig-build.json",
    "build-doc": "typedoc src",
    "build-es": "ttsc --project tsconfig-build-es.json",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "test": "jest",
    "test-fast": "jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,prod --reject @types/node",
    "update-package-check-peer": "ncu --dep peer --minimal --reject @types/node",
    "update-package-check-skylib": "ncu --dep peer --filter @skylib/*",
    "update-package-force": "ncu --dep dev,prod --reject @types/node --upgrade",
    "update-package-force-peer": "ncu --dep peer --minimal --reject @types/node --upgrade",
    "update-package-force-skylib": "ncu --dep peer --filter @skylib/* --upgrade"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@jest/test-sequencer": "^27.4.6",
    "@skylib/config": "^0.1.30",
    "@skylib/eslint-plugin": "^0.1.8",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^8.7.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^40.1.0",
    "eslint-plugin-vue": "^8.4.0",
    "jest": "^27.4.7",
    "jest-environment-jsdom": "^27.4.6",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-environment-node": "^27.4.6",
    "jest-mock-axios": "^4.5.0",
    "jest-transform-stub": "^2.0.0",
    "npm-check-updates": "^12.2.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-transform-import-path-rewrite": "^0.3.0",
    "ttypescript": "^1.5.13",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.12",
    "typescript": "^4.5.5",
    "vue-eslint-parser": "^8.2.0"
  },
  "peerDependencies": {
    "@skylib/facades": "^0.1.20",
    "@skylib/functions": "^0.1.18",
    "@skylib/typescript-types": "^0.1.1",
    "@types/facebook-js-sdk": "^3.3.5",
    "@types/gapi.auth2": "^0.0.56",
    "@types/jquery": "^3.5.10",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.5",
    "@types/lunr": "^2.3.4",
    "@types/natural-compare": "^1.4.1",
    "@types/pouchdb": "^6.4.0",
    "@types/pouchdb-adapter-memory": "^6.1.3",
    "@types/sha256": "^0.2.0",
    "@types/uuid": "^8.3.3",
    "axios": "^0.25.0",
    "date-fns": "^2.27.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lunr": "^2.3.9",
    "minisearch": "^3.2.0",
    "natural-compare": "^1.4.0",
    "pouchdb": "npm:@skylib/fix-pouchdb@^0.1.1",
    "pouchdb-adapter-memory": "^7.2.2",
    "pouchdb-collate": "^7.2.2",
    "sha256": "^0.2.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
