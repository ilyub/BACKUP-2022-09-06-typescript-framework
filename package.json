    "typescript": "^4.6.3",
    "vue-eslint-parser": "^8.3.0"
  },
  "peerDependencies": {
    "@skylib/facades": "^2.0.0",
    "@skylib/functions": "^1.3.3",
    "@skylib/lodash-commonjs-es": "^0.1.0",
    "@skylib/typescript-types": "^1.0.0",
    "@types/facebook-js-sdk": "^3.3.5",
    "@types/gapi.auth2": "^0.0.56",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.179",
    "@types/lodash-es": "^4.17.6",
    "@types/lunr": "^2.3.4",
    "@types/natural-compare": "^1.4.1",
    "@types/pouchdb": "^6.4.0",
